{
  "name": "@monsantoit/c7-bpsm-ui",
  "version": "1.5.0",
  "description": "Brand Portal Support Manager - Internal C7 Administration Application",
  "author": "gcnmn <gcnmn@monsanto.com>",
  "private": false,
  "license": "UNLICENSED",
  "config": {
    "externalUserAdminUrl": "https://dev-mycrop.bayer.com/admin",
    "internalUserAdminUrl": "https://bpsm-np.monsanto.net/c7-bpsm"
  },
  "scripts": {
    "dev:external": "BASE_URL=$npm_package_config_externalUserAdminUrl webpack-dev-server --config ./webpack/webpack.external.dev.js",
    "dev:internal": "BASE_URL=$npm_package_config_internalUserAdminUrlL webpack-dev-server --config ./webpack/webpack.internal.dev.js",
    "start": "npm run build && PUBLIC_PATH='/' node server/index.js",
    "unit": "jest --config test/unit/jest.conf.js",
    "unit:nocache": "jest --config test/unit/jest.conf.js --no-cache",
    "test": "npm run unit",
    "test:visual:medium": "VIEWPORT_SIZE=medium BATCH_ID=$GIT_COMMIT mocha --timeout 60000 test/e2e/specs",
    "test:visual:large": "VIEWPORT_SIZE=large BATCH_ID=$GIT_COMMIT mocha --timeout 60000 test/e2e/specs",
    "test:visual:xlarge": "VIEWPORT_SIZE=xlarge BATCH_ID=$GIT_COMMIT mocha --timeout 60000 test/e2e/specs",
    "test:visual:medium:local": "LOCAL_TESTING=true VIEWPORT_SIZE=medium mocha --timeout 60000 test/e2e/specs",
    "test:visual:large:local": "LOCAL_TESTING=true VIEWPORT_SIZE=large mocha --timeout 60000 test/e2e/specs",
    "test:visual:xlarge:local": "LOCAL_TESTING=true VIEWPORT_SIZE=xlarge mocha --timeout 60000 test/e2e/specs",
    "lint": "eslint --ext .js,.ts,.vue src test/unit",
    "build": "npm run build:external && npm run build:internal",
    "build:external": "BASE_URL=$npm_package_config_externalUserAdminUrl webpack --config webpack/webpack.external.js",
    "build:internal": "BASE_URL=$npm_package_config_internalUserAdminUrl webpack --config webpack/webpack.internal.js"
  },
